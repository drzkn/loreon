{
  "name": "loreon",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@1.22.18",
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:ci": "NODE_ENV=production next build",
    "build:vercel": "node scripts/verify-deps.js && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:cache": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:connection": "npx tsx src/examples/config.example.ts",
    "test:e2e": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:slow": "playwright test --headed --slowMo=1000",
    "test:e2e:demo": "playwright test --project=demo",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "example:supabase": "npx tsx src/examples/supabaseExample.ts",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "clean:cache": "rm -rf .next/cache",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/openai": "^1.3.23",
    "@ai-sdk/react": "^1.2.12",
    "@huggingface/inference": "^4.5.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.2",
    "@tailwindcss/postcss": "^4.1.11",
    "ai": "^4.3.19",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "dotenv": "^17.0.0",
    "lucide-react": "^0.525.0",
    "next": "15.3.4",
    "openai": "^5.10.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^9.0.14",
    "styled-components": "^6.1.19",
    "supabase": "^2.26.9",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-a11y": "^9.0.14",
    "@storybook/addon-docs": "^9.0.14",
    "@storybook/addon-onboarding": "^9.0.14",
    "@storybook/addon-vitest": "^9.0.14",
    "@storybook/nextjs": "^9.0.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/styled-components": "^5.1.34",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-storybook": "^9.0.14",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
